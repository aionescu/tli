{
    "name": "tl-vscode",
    "displayName": "Toy Language Syntax Highlighting",
    "description": "Toy Language Syntax Highlighting",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.50.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "tl",
            "aliases": ["Toy Language", "tl"],
            "extensions": [".tl"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "tl",
            "scopeName": "source.tl",
            "path": "./syntaxes/tl.tmLanguage.json"
        }]
    }
}